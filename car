class Car;

  def_init_(self,rego,ownerName);
    self.rego = rego
    self.ownerName = ownerName
  
  def getOwner(self):
    return self.ownerName
    
  def getRego(self):
  return self.rego

class ParkingLot:

 def_init_(self,sid,isStaff):
   self.sid=sid
   self.isOccupied=False
   self.isStaff=isStaff
   
 def getParkingID(self):
   return self.sid
   
 def getisStaff(self):
   return self.isStaff
   
 def getVehicle(self):
   return self.carinparking
   
